{
  "name": "@powerhome/eslint-config",
  "version": "0.2.2",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./base": "./lib/base.js",
    "./flow": "./lib/flow.js",
    "./typescript": "./lib/typescript.js",
    "./prettier": "./lib/prettier.js",
    ".": "./lib/typescript.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "scripts": {
    "lint": "eslint lib/*",
    "prettier": "prettier -c --config ./lib/prettier.js .",
    "prettier:write": "prettier --write --config ./lib/prettier.js .",
    "expose-readme": "cp docs/README.md ."
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-eslint": "8 || 10",
    "eslint": "7.32.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-jsx-control-statements": "3.0.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "4.0.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-jsx-control-statements": "3.0.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "4.0.1",
    "prettier": "2.8.8",
    "semver": "7.5.4",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.6.0"
  }
}
